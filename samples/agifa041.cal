|PROCESO PonPoblacion;
     aAlfa   = #agifa041#3;  |QBF aAlfa;
     aLong   = aAlfa % 0;
     nLong   = aLong;
     aCodPos = "";
     aPobla  = "";
     nEstoy  = 0;
     |PARA nCarac = 1;  |SI nCarac [ nLong;  |HACIENDO nCarac = nCarac + 1;
           nPos = (nCarac * 100) + 1;

           aCarac = aAlfa % nPos;

           |SI nEstoy = 0;
               |SI aCarac ] "0";  |Y aCarac [ "9";
                   aCodPos = aCodPos + aCarac;
               |SINO;
                   nEstoy = 1;
               |FINSI;
           |FINSI;

           |SI nEstoy = 1;
               |SI aCarac ! " ";  |Y aCarac ! "-";
                   nEstoy = 2;
               |FINSI;
           |FINSI;

           |SI nEstoy = 2;
               aPobla = aPobla + aCarac;
           |FINSI;
     |SIGUE;

     #agifa280#13 = aCodPos;
     #agifa280#14 = aPobla;
|FPRC;


