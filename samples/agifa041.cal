|PROCESO Version; |TIPO 7;
     enSw = 0;
     |VERSIONRTME aAlfa1;
     aAlfa2 = aAlfa1 % 101;
     |SI aAlfa2 = "8";                       || diagram8
          enSw = 1;
     |FINSI;
     |SI aAlfa2 = "9";|Y aAlfa1 < "9.09V";   || diagram9 inferior a 9.09V-1
          enSw = 1;
     |FINSI;
     |SI enSw = 1;
         |MENAV "      Version de Basico inferior a la requerida (9.09V-1)";
     |FINSI;
|FPRC;

|CALCULO Tipo20; |TIPO 20;
     aAlfa1 = FSalida;
     |SI aAlfa1 = "OPCION";
