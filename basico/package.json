{
    "name": "basico",
    "displayName": "BASICO",
    "description": "BASICO language extension for VSCode",
    "version": "0.0.1",
    "publisher": "Simple2B",
    "repository": {
        "url": "https://github.com/Simple2B/vscode.ext.git",
        "directory": "basico",
        "type": "git"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "basico",
            "aliases": ["BASICO", "basico", "cal"],
            "extensions": [".cal"],
            "filenames": [],
            "configuration": "./basico/language.json"
        }],
        "grammars": [{
            "language": "basico",
            "scopeName": "source.cal",
            "path": "./basico/syntax.json"
        }]
    }
}